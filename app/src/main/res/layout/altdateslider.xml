<?xml version="1.0" encoding="utf-8"?><com.rbais.android.widgets.DateRibbon.SliderContainer xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/dateRibbonContainer"    android:layout_width="fill_parent"    android:layout_height="fill_parent">    <com.rbais.android.widgets.DateRibbon.ScrollLayout        style="@style/Scroller"        app:labelerClass="com.rbais.android.widgets.DateRibbon.labeler.YearLabeler"        app:labelerFormat="%tY"        app:childWidth="180dp" />    <com.rbais.android.widgets.DateRibbon.ScrollLayout        style="@style/Scroller"        app:labelerClass="com.rbais.android.widgets.DateRibbon.labeler.MonthLabeler"        app:labelerFormat="%tB"        app:childWidth="160dp" />    <com.rbais.android.widgets.DateRibbon.ScrollLayout        style="@style/Scroller"        app:labelerClass="com.rbais.android.widgets.DateRibbon.labeler.DayDateLabeler"        app:labelerFormat="%td %ta"        app:childWidth="45dp" />    <include layout="@layout/dialogbuttons" /></com.rbais.android.widgets.DateRibbon.SliderContainer>