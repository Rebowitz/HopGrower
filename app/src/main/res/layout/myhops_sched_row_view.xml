<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="horizontal"    android:paddingBottom="10dp">    <TextView        android:id="@+id/interval"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginRight="30dp"        android:text=""        android:textSize="20sp"        android:textColor="#000000"        android:layout_weight="1" />    <TextView        android:id="@+id/startdate"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginRight="30dp"        android:text=""        android:textSize="20sp"        android:textColor="#000000"        android:layout_weight="1" />    <TextView        android:id="@+id/event"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginRight="30dp"        android:text=""        android:textSize="18sp"        android:textColor="#000000"        android:layout_weight="1" />    <TextView        android:id="@+id/event_other"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginRight="30dp"        android:text=""        android:textSize="18sp"        android:textColor="#000000"        android:layout_weight="1" />    <TextView        android:id="@+id/schedId"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:visibility="invisible"        android:layout_weight="0" /></LinearLayout> 	 