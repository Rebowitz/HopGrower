<?xml version="1.0" encoding="utf-8"?><com.rbais.android.widgets.DateRibbon.SliderContainer xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/dateRibbonContainer"    android:layout_width="fill_parent"    android:layout_height="fill_parent">    <com.rbais.android.widgets.DateRibbon.ScrollLayout        style="@style/Scroller"        app:labelerClass="com.rbais.android.widgets.DateRibbon.labeler.HourLabeler"        app:labelerFormat="%tk"        app:childWidth="90dp" />    <com.rbais.android.widgets.DateRibbon.ScrollLayout        style="@style/Scroller"        app:labelerClass="com.rbais.android.widgets.DateRibbon.labeler.MinuteLabeler"        app:labelerFormat="%tM"        app:childWidth="45dp" />    <include layout="@layout/dialogbuttons" /></com.rbais.android.widgets.DateRibbon.SliderContainer>