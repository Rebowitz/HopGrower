<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    android:paddingBottom="10dp"    android:weightSum="5">    <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:orientation="horizontal">        <TextView            android:id="@+id/glID"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:padding="10dp"            android:text="@string/growlog"            android:textSize="22sp"            android:textStyle="bold"            android:textColor="#000000" />        <TextView            android:id="@+id/growdate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:padding="10dp"            android:text=""            android:textSize="20sp"            android:textColor="#000000" />    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal">        <TextView            android:id="@+id/growcat"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginRight="10dp"            android:text=""            android:textSize="20sp"            android:textColor="#000000" />        <TextView            android:id="@+id/growtype"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginRight="10dp"            android:text=""            android:textSize="18sp"            android:textColor="#000000" />        <TextView            android:id="@+id/growdesc"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginRight="10dp"            android:text=""            android:textSize="18sp"            android:textColor="#000000" />    </LinearLayout></LinearLayout> 	 