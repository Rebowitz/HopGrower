<?xml version="1.0" encoding="utf-8"?><ScrollView xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/ScrollView01"    android:layout_width="fill_parent"    android:layout_height="fill_parent">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="#10DA5C">        <TextView            android:id="@+id/hu_info_page"            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_alignParentTop="true"            android:padding="10dp"            android:layout_marginBottom="16dp"            android:text="@string/hopunion"            android:textSize="16sp"            android:background="#10DA5C" />        <TextView            android:id="@+id/textView1"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_alignParentRight="true"            android:layout_alignParentTop="true"            android:layout_below="@+id/hu_info_page"            android:layout_marginTop="36dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="22sp"            android:textStyle="bold" />        <TextView            android:id="@+id/aroma"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/textView1"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/usage"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/aroma"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/substitute"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/usage"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/styles"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/substitute"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <!-- divider 1 -->        <TextView            android:id="@+id/lineA"            android:layout_width="fill_parent"            android:layout_height="2dp"            android:layout_below="@+id/styles"            android:layout_marginTop="10dp"            android:background="#000000" />        <TextView            android:id="@+id/pedigree"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/lineA"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/yield"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/pedigree"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/storage"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/yield"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/info"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/storage"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <TextView            android:id="@+id/perception"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/info"            android:layout_marginTop="10dp"            android:layout_marginLeft="10dp"            android:text="@string/dbinfo"            android:textSize="16sp" />        <!-- divider 2 -->        <TextView            android:id="@+id/lineB"            android:layout_width="fill_parent"            android:layout_height="2dp"            android:layout_marginTop="10dp"            android:layout_below="@+id/perception"            android:background="#000000" />        <!-- 		 <GridView                        android:id="@+id/var_info_grid"                        android:layout_below="@+id/lineB"                        android:layout_width="fill_parent"                        android:layout_height="fill_parent"                        android:numColumns="2"                        android:verticalSpacing="10dp"                        android:layout_marginLeft="10dp"                        android:gravity="center"                        android:layout_alignParentBottom="true"                        android:background="@android:color/transparent"                        android:stretchMode="columnWidth" >                </GridView>        -->        <com.rbais.expandablewidgets.ExpandableHeightGridView            android:id="@+id/var_info_grid"            android:layout_below="@+id/lineB"            android:layout_width="fill_parent"            android:layout_height="fill_parent"            android:numColumns="2"            android:verticalSpacing="10dp"            android:layout_marginLeft="10dp"            android:gravity="center"            android:layout_alignParentBottom="true"            android:background="@android:color/transparent"            android:stretchMode="columnWidth" />    </RelativeLayout></ScrollView>