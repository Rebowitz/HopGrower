<?xml version="1.0" encoding="utf-8"?><PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">    <PreferenceCategory android:title="Personal Information">        <EditTextPreference            android:title="Your Name"            android:key="username"            android:summary="Please provide your username"></EditTextPreference>        <EditTextPreference            android:title="Your E-mail Address"            android:key="email_addy"            android:summary="Please provide your e-mail address"></EditTextPreference>        <EditTextPreference            android:title="Your E-mail Password"            android:key="email_pass"            android:summary="Please provide your e-mail account password"></EditTextPreference>    </PreferenceCategory>    <PreferenceCategory android:title="Application Configuration">        <CheckBoxPreference            android:title="AutoUpdate Application - Yes/No"            android:defaultValue="false"            android:key="appUpdatePref"            android:summaryOn="Yes"            android:summaryOff="No" />        <ListPreference            android:title="Application Update Interval"            android:key="listPref"            android:defaultValue="1"            android:entries="@array/listArray"            android:entryValues="@array/listValues" />        <CheckBoxPreference            android:title="AutoUpdate App Data - Yes/No"            android:defaultValue="false"            android:key="appDataPref"            android:summaryOn="Yes"            android:summaryOff="No" />        <CheckBoxPreference            android:title="AutoUpdate KnowledgeBase Data - Yes/No"            android:defaultValue="false"            android:key="kbDataPref"            android:summaryOn="Yes"            android:summaryOff="No" />        <CheckBoxPreference            android:title="Data Storage - Local/SD Card"            android:defaultValue="false"            android:key="dataStoragePref"            android:summaryOn="Local Storage"            android:summaryOff="SD Card" />        <CheckBoxPreference            android:title="Download over WI-FI only"            android:defaultValue="false"            android:key="dwnldWiFiPref"            android:summaryOn="Wi-Fi only"            android:summaryOff="Any connection" />    </PreferenceCategory>    <PreferenceCategory android:title="Application Support/Feedback">        <CheckBoxPreference            android:title="AutoSubmit Error Data - Yes/No"            android:defaultValue="false"            android:key="dwnldWiFiPref"            android:summaryOn="Yes"            android:summaryOff="No" />    </PreferenceCategory></PreferenceScreen>